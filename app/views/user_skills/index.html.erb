<div class="row">
  <div class="col-md-6">
    <div class="card">
      <div class="header">
        <div style="float:left;"><h4 class="title">My Skills</h4></div>
      </div>

      <table class="table table-hover table-striped">
        <thead>
        <tr>
          <th>Skill</th>
          <th>Total Hours</th>

        </tr></thead>
        <tbody>
        <% @results.each do |a| %>
            <tr>
              <td><%= Skill.find(a["skillid"]).skill_title %></td>
              <td><%= a["totalhours"].to_i %></td>

            </tr>
        <% end %>
        </tbody>
      </table>

    </div>
  </div>


</div>