<div class="col-md-8">
  <div class="card">

    <div class="header">
      <p style="float:right;"><a href="/"><i class="pe-7s-angle-left-circle"></i></a></p>
      <h4 class="title">Edit Profle</h4>

    </div>


    <div class="content">
      <%= bootstrap_form_for @user do |f| %>

          <%= f.text_field :first_name%>
          <%= f.text_field :last_name%>
          <%= f.text_area :summary%>
          <%= f.text_field :location%>
          <%= f.email_field :email%>
          <%= f.phone_field :contact_no%>
          <%= f.text_field :website%>
          <%= f.submit "Save", :class => "btn btn-default"%>

      <% end %>
    </div>
  </div>
</div>